{
  "$schema": "http://json-schema.org/schema#",
  "id": "/db/MetricData",
  "type": "object",
  "properties": {
    "metric_uid": {
      "type": "string",
      "maxLength": 50,
      "minLength": 1
    },
    "timestamp": {
      "type": "string"
    },
    "metric_value": {
      "type": "number"
    }
  },
  "additionalProperties": false,
  "required": ["metric_uid", "timestamp", "metric_value"]
}
